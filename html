// inicial 

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ma Premiere Page HTML</title>
    <link rel="stylesheet" href="1.css">
    <style>
        /* Estilo para a página */
        body {
            margin: 0;
            padding: 0;
            background-image: url(im.avif); 
            background-repeat: no-repeat;
            background-position: center;
            background-attachment: fixed;
            background-size: cover;
            font-family: "roboto", sans-serif;
        }

        /* Estilo para o cabeçalho */
        header {
            background-color: #333;
            color: #fff;
            padding: 10px;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        /* Estilo para o logo */
        .logo {
            width: 60px;
            text-decoration: none;
        }

        /* Estilo para a barra de navegação */
        nav {
            display: flex;
            position: sticky;
            top: 0;
            z-index: 9999;
            
        }

        /* Estilo para os links */
        #link {
            list-style: none;
            margin: 0;
            padding: 0;
            display: flex;
            gap: 20px;
        }

        /* Estilo para os links individuais */
        #link li {
            color: #fff;
            padding: 10px;
            margin: 10px;
        }
        #link li a{
            text-decoration: none;
        }
        #link li a:hover{
            color:crimson;
        }

        /* Bloco centralizado no meio da página */
        .content-block {
            text-align: center;
            margin: 50px auto;
            padding: 20px;
            background-color: #f2f2f2;
            width: 80%;
            max-width: 600px;
            border-top: 2px solid white;
            margin-bottom: 10px;
            border: none   ;
        }

        /* Estilos para o h1 e o p no bloco centralizado */
        .content-block h1 {
            font-size: 24px;
            line-height: 120%;

            color: #333;
        }

        .content-block p {
            font-size: 16px;
            color: #555;
        }
    </style>
</head>
<body>
    <header>
        <a href="#" class="logo">
            <img src="images.png" alt="Logo da sua empresa">
        </a>
