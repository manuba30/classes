    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
    <link rel="stylesheet" href="6up.css">
    <script src="https://kit.fontawesome.com/f09d67eb93.js" crossorigin="anonymous"></script>
</head>

<body>
    <!--Contenu de la page-->

    <main>
        
        <section id="ancre-contact" class="contact">
