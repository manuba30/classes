@keyframes moveSquares {
    0% {
        transform: translateX(0);
    }
    50% {
        transform: translateX(calc(-100vw - 50px)); /* Mover completamente para fora da tela para o square de cima */
    }
    100% {
        transform: translateX(calc(-100vw - 50px)); /* Mover completamente para fora da tela para o square de cima */
    }
}

.contact-form-content:hover .square-contact-form-1 {
    animation: moveSquares 1s linear forwards; /* Adicionando "forwards" para manter o estado final */
}

@keyframes moveSquaresReverse {
    0% {
        transform: translateX(0);
    }
    50% {
        transform: translateX(calc(100vw + 50px)); /* Mover completamente para fora da tela para o square de baixo */
    }
    100% {
        transform: translateX(calc(100vw + 50px));
    }
}

.contact-form-content:hover .square-contact-form-2 {
    animation: moveSquaresReverse 1s linear forwards; /* Adicionando "forwards" para manter o estado final */
}








